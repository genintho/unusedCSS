{
    "name": "xtUnusedCSS",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Find unused CSS Selector from your Web site/Web application",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "just click to inject jquery 1.7"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [ "tabs", "https://*/*", "http://*/*" ]
}