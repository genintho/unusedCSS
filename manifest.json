{
    "name": "unusedCSS",
    "version": "0.3.1",
    "manifest_version": 2,
    "description": "Find unused CSS Selector from your Web site/Web application just by using it",
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_title": "Reduce your CSS"
    },
    "background": {
        "scripts": [
            "object/selector.js",
            "object/domain.js",
            "background.js"
        ]
    },
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "permissions": [
        "tabs",
        "notifications",
        "https://*/*",
        "http://*/*"
    ],
    "homepage_url": "http://thomas-genin.com/unusedcss.html"
}